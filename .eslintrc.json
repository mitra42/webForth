{
    "env": {
        "browser": true,
        "jest/globals": true
    },
    "extends": "airbnb",
    "plugins": [
        "babel",
        "eslint-plugin-import",
        "eslint-plugin-react",
        "jest",
        "jsx-a11y"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2020
    },
    "rules": {
        "brace-style": 0,
        "camelcase": 0,
        "class-methods-use-this": 0,
        "function-paren-newline": 0,
        "max-classes-per-file": 0,
        "guard-for-in": 1,
        "import/extensions": 0,
        "lines-between-class-members": 0,
        "max-len": 0,
        "no-await-in-loop": 0,
        "no-bitwise": 0,
        "no-cond-assign": 0,
        "no-confusing-arrow": 0,
        "no-console": 0,
        "no-else-return": 0,
        "no-multi-spaces": 0,
        "no-nested-ternary": 0,
        "no-param-reassign": 0,
        "no-plusplus": 0,
        "no-restricted-globals": 0,
        "no-return-assign": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": 1,
        "no-useless-escape": 0,
        "no-restricted-syntax": 1,
        "object-curly-newline": 0,
        "object-property-newline": 0,
        "prefer-destructuring": 0,
        "prefer-template": 0,
        "spaced-comment": 0
    }
}
